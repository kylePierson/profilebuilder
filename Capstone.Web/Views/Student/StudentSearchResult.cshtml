@model List<Capstone.Data.Models.Student>

@{
    Layout = null;
}

    <div class="row">

        <div class="col-md-9">

            <h2>Search Result</h2>
            <div>
                <ul>
                    @foreach (var student in Model)
                    {
                        <li><img src="@string.Concat("/Content/images/", student.FirstName.ToLower(), student.LastName.ToLower(), ".jpg")" class="img-circle profileImage" /></li>
                        <li>@student.FirstName  @student.LastName</li>
                    }
                </ul>
            </div>
        </div>
    </div>
    

